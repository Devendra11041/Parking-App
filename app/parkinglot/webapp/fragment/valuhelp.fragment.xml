<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Products"
		items="{/PlotNOs}"
		search=".onValueHelpDialogSearch"
		confirm=".onValueHelpDialogClose"
		cancel=".onValueHelpDialogClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{plot_NO}"
			description="{plot_NO}" />
	</SelectDialog>
</core:FragmentDefinition>